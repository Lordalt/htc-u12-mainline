&pio {
 disp_rst_pins: lcm-pins {
  pins-rst {
   pinmux = <(((106) << 8) | 0)>;
  };
 };

 mmc0_pins_default: emmc-sdr-pins {
  pins-cmd-dat {
   pinmux = <(((154) << 8) | 1)>,
     <(((155) << 8) | 1)>,
     <(((156) << 8) | 1)>,
     <(((157) << 8) | 1)>,
     <(((158) << 8) | 1)>,
     <(((159) << 8) | 1)>,
     <(((160) << 8) | 1)>,
     <(((161) << 8) | 1)>,
     <(((162) << 8) | 1)>;
   input-enable;
   bias-pull-up = <101>;
  };

  pins-clk {
   pinmux = <(((163) << 8) | 1)>;
   bias-pull-down = <102>;
  };

  pins-rst {
   pinmux = <(((165) << 8) | 1)>;
   bias-pull-up = <102>;
  };
 };

 mmc0_pins_uhs: emmc-uhs-pins {
  pins-cmd-dat {
   pinmux = <(((154) << 8) | 1)>,
     <(((155) << 8) | 1)>,
     <(((156) << 8) | 1)>,
     <(((157) << 8) | 1)>,
     <(((158) << 8) | 1)>,
     <(((159) << 8) | 1)>,
     <(((160) << 8) | 1)>,
     <(((161) << 8) | 1)>,
     <(((162) << 8) | 1)>;
   input-enable;
   drive-strength = <6>;
   bias-pull-up = <101>;
  };

  pins-clk {
   pinmux = <(((163) << 8) | 1)>;
   drive-strength = <6>;
   bias-pull-down = <102>;
  };

  pins-rst {
   pinmux = <(((165) << 8) | 1)>;
   drive-strength = <6>;
   bias-pull-up = <102>;
  };

  pins-ds {
   pinmux = <(((164) << 8) | 1)>;
   drive-strength = <6>;
   bias-pull-down = <102>;
  };
 };

 nfc_pins: nfc-pins {
  pins-irq {
   pinmux = <(((3) << 8) | 0)>;
   bias-pull-down;
   input-enable;
  };

  pins-fw-ven {
   pinmux = <(((94) << 8) | 0)>,
     <(((149) << 8) | 0)>;
  };
 };

 ts_pins: touchscreen-pins {
  pins-irq {
   pinmux = <(((6) << 8) | 0)>;
   bias-pull-up;
   input-enable;
  };

  pins-rst {
   pinmux = <(((102) << 8) | 0)>;
   output-high;
  };
 };

 proximity_pins: proximity-pins {
  pins-irq {
   pinmux = <(((8) << 8) | 0)>;
   bias-pull-up;
   input-enable;
  };
 };

 accel_pins: accelerometer-pins {
  pins-irq {
   pinmux = <(((12) << 8) | 0)>;
   bias-pull-up;
   input-enable;
  };
 };

 i2c0_pins: i2c0-pins {
  pins-bus {
   pinmux = <(((45) << 8) | 1)>,
     <(((46) << 8) | 1)>;
   input-enable;
  };
 };

 i2c1_pins: i2c1-pins {
  pins-bus {
   pinmux = <(((125) << 8) | 1)>,
     <(((126) << 8) | 1)>;
   bias-disable;
  };
 };

 i2c2_pins: i2c2-pins {
  pins-bus {
   pinmux = <(((43) << 8) | 1)>,
     <(((44) << 8) | 1)>;
   bias-disable;
  };
 };

 i2c3_pins: i2c3-pins {
  pins-bus {
   pinmux = <(((136) << 8) | 1)>,
     <(((137) << 8) | 1)>;
   bias-disable;
  };
 };

 i2c4_pins: i2c4-pins {
  pins-bus {
   pinmux = <(((100) << 8) | 4)>,
     <(((101) << 8) | 4)>;
   bias-disable;
  };
 };

 uart0_pins: uart0-pins {
  pins-rx {
   pinmux = <(((113) << 8) | 1)>;
   bias-pull-up;
   input-enable;
  };
  pins-tx {
   pinmux = <(((114) << 8) | 1)>;
   output-high;
  };
 };

 uart2_pins: uart2-pins {
  pins-rx {
   pinmux = <(((31) << 8) | 1)>;
   bias-pull-up;
   input-enable;
  };
  pins-tx {
   pinmux = <(((32) << 8) | 1)>;
  };
 };
};

