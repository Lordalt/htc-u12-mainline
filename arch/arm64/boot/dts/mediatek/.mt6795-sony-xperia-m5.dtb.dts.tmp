# 0 "arch/arm64/boot/dts/mediatek/mt6795-sony-xperia-m5.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/mediatek/mt6795-sony-xperia-m5.dts"






/dts-v1/;
